{
  "name": "amphibee/studizz",
  "description": "A Laravel package to interact with the Studizz API",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "12.41.1",
    "illuminate/container": "12.41.1",
    "illuminate/contracts": "12.41.1",
    "illuminate/http": "12.41.1"
  },
  "autoload": {
    "psr-4": {
      "AmphiBee\\Studizz\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AmphiBee\\Studizz\\Providers\\StudizzServiceProvider"
      ],
      "aliases": {
        "Campus": "AmphiBee\\Studizz\\Providers\\Campus",
        "Contact": "AmphiBee\\Studizz\\Providers\\Contact",
        "Formation": "AmphiBee\\Studizz\\Providers\\Formation"
      }
    }
  },
  "require-dev": {
    "pestphp/pest": "^4.0",
    "mockery/mockery": "^1.6",
    "laravel/pint": "^1.16"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
